  "license": "MIT",
  "scripts": {
    "start:server": "NODE_ENV=development nodemon --config server/nodemon.json --exec node --harmony-async-await server/index.js",
    "start:client": "NODE_ENV=development nodemon --config client/nodemon.json --exec node_modules/.bin/webpack-dev-server -- --config client/webpack.config.js",
    "test": "npm run test:server && npm run test:client",
    "test:server": "NODE_ENV=test node --harmony-async-await node_modules/.bin/ava server/**/*.test.js",
    "test:client": "NODE_ENV=test ava server/**/*.test.js"
  },
  "dependencies": {
    "ava": "^0.18.2",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "enzyme": "^2.7.1",
    "eslint-plugin-react": "^6.10.0",
    "file-loader": "^0.10.0",
    "freezer-js": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "koa": "next",
    "koa-bodyparser": "next",
    "koa-cors": "^0.0.16",
    "koa-router": "next",
    "lodash": "^4.17.4",
    "lokijs": "^1.4.3",
    "nodemon": "^1.11.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "next",
    "supertest": "^3.0.0",
    "url-loader": "^0.5.7",
    "uuid": "^3.0.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "devDependencies": {
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-standard": "^2.0.1"
  }
}
